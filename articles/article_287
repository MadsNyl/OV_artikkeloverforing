        "header": "Kraftfulle kvinner på Sarabråten",
        "ingress": {
            "image": {
                "url": "https://www.ostmarkasvenner.no/getfile.php/2009931.2128.pyddqabwbw/Kraftfulle+kvinner+Sarabr%C3%A5ten+2012.jpg",
